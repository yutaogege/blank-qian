extends ./layout/layout
append head
	title Blank ME
	link(rel='stylesheet',href='css/style.css')
block content
	//- 导航
	div.header
		div.header-wrap
			a.logo(href="#"): span
			ul.nav.nav-lv1
				li: a.single-nav(href="#") HOME
					span.line
				li
					a.single-nav(href="#") PRODUCTS
						span.line
					div.nav-mask
					ul.nav-lv2.n-products
						li: a(href="#") nav11111
						li: a(href="#") nav2222
						li: a(href="#") nav3333
				li
					a.single-nav(href="#") BRAND
						span.line
					//- div.nav-mask
					//- ul.nav-lv2.n-brand
					//- 	li: a(href="#") nav11111
					//- 	li: a(href="#") nav2222
					//- 	li: a(href="#") nav3333
				li
					a.single-nav.last(href="#") CONTANT
						span.line
					div.n-contant
			div.min-nav-wrap
				a.min-nav-btn.J_nav_btn(href="javascript:;")
				ul.nav.nav-min-lv1
					li: a.single-nav(href="#") PRODUCTS
					li: a.single-nav(href="#") BRAND
					li.last: a.single-nav(href="#") CONTANT

	//- banner
	div.banner(style="background-image:url(./images/banner-pc.jpg)")

	//- muted
	//- 视频
	div.video-wrap
		video.video(loop="loop" muted)
			source(src="source/video.mp4" type="video/mp4")
	div.slogan-mod
		div.slogan-fix
			span(style="background-image:url(./images/f-text.png)")
